{
  "name": "wurwolves",
  "version": "0.0.1",
  "private": true,
  "description": "A simple werewolves (/ mafia) webapp using a React+Redux frontend with a Python+FastAPI+SQLAlchemy backend, served by a static node server",
  "scripts": {
    "start": "run-script-os",
    "start:win32": "concurrently \"node server\" \"uvicorn backend.main:app\"",
    "start:default": "concurrently \"node server\" \"uvicorn backend.main:app --host 0.0.0.0\"",
    "build": "run-script-os",
    "build:win32": "alembic upgrade head && cd react-ui && npm run build",
    "build:default": "alembic upgrade head && cd react-ui && npm run build",
    "install": "cd react-ui/ && npm install"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.16.4",
    "run-script-os": "^1.1.1",
    "http-proxy-middleware": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlesbaynham/wurwolves.git"
  },
  "keywords": [
    "node",
    "heroku",
    "fastapi",
    "react",
    "wurwolves"
  ],
  "license": "None",
  "devDependencies": {}
}
